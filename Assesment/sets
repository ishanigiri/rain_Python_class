# Step 1: Create a set 
my_set = {1, 2, 2, 3, 4, 4, 5}
print("Initial set:", my_set)

# Step 2: Adding element 6 to the set
my_set.add(6)
print("After adding 6:", my_set)

# Step 3: Removing element 3 from the set
my_set.discard(3)
print("After removing 3:", my_set)

# Step 4: Print all elements of the set
print("Current elements in the set:", my_set)

# Step 5: Check if 4 is in the set
if 4 in my_set:
    print("Is 4 in the set? True")
else:
    print("Is not 4 in the set? False")
