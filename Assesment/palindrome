def is_palindrome(s):
    # Reverse the string
    reversed_string = s[::-1]
    # Check if the original and reversed strings are the same
    if s == reversed_string:
        return True
    else:
        return False
print(is_palindrome("aca"))     
print(is_palindrome("aabbaa")) 
print(is_palindrome("abbbb"))   
print(is_palindrome("baabbb"))  
